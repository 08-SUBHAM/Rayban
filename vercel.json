{
  "version": 2,
  "builds": [
    {
      "src": "public/*.{html,css,js,png,jpg,jpeg}",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/(.*.html)", "dest": "/public/$1" },
    { "src": "/(.*.css)", "dest": "/public/$1" },
    { "src": "/(.*.js)", "dest": "/public/$1" },
    { "src": "/(.*.(png|jpg|jpeg))", "dest": "/public/$1" },
    { "src": "/(.*)", "dest": "/public/Index.html" }
  ]
}
